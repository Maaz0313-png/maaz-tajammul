{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|fonts|svg|favicon.ico|robots.txt|apple-touch-icon.png|logo.svg)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:ZGiyXVRwNq/0iO9DYufqJyyw7HE6CIHpv18APhFlVfc=",
        "APP_URL": "https://maaztajammul.me",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "build": {
        "env": {
            "COMPOSER_MIRROR_PATH_REPOS": "1"
        }
    },
    "buildCommand": "npm run build:ssr",
    "devCommand": "php artisan serve"
}
